<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
  <managed-bean>
    <managed-bean-name>bidBean1</managed-bean-name>
    <managed-bean-class>coreservlets.BidBean1</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>bidBean2</managed-bean-name>
    <managed-bean-class>coreservlets.BidBean2</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/bid1.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/WEB-INF/results/showBid1.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/bid2.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/WEB-INF/results/showBid2.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/bid3.jsp</from-view-id>
    <navigation-case>
      <from-outcome>success</from-outcome>
      <to-view-id>/WEB-INF/results/showBid3.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
    <navigation-rule>
      <from-view-id>/bid4.jsp</from-view-id>
      <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/WEB-INF/results/showBid4.jsp</to-view-id>
      </navigation-case>
  </navigation-rule>
</faces-config>
